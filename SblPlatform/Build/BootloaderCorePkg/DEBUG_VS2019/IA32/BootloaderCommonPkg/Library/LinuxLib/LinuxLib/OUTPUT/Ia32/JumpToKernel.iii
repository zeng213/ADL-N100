  SECTION .text

;------------------------------------------------------------------------------
; void
; __cdecl
; JumpToKernel (
;   void *KernelStart,
;   void *KernelBootParams
;   );
;------------------------------------------------------------------------------
global _JumpToKernel
_JumpToKernel:
    mov     esi, [esp + 8]
    call    DWORD [esp + 4]
    ret

;------------------------------------------------------------------------------
; void
; __cdecl
; JumpToKernel64 (
;   void *KernelStart,
;   void *KernelBootParams
;   );
;------------------------------------------------------------------------------
global _JumpToKernel64
_JumpToKernel64:
    DB      0x67, 0x48                ; 32-bit address size, 64-bit operand size
    mov     esi, edx                  ; mov esi, rdx
    call    ecx
    hlt
