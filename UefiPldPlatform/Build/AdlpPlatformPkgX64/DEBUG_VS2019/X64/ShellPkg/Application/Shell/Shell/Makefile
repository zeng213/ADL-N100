	$(CP) e:\code\adl_n100_code\uefipldplatform\Build\AdlpPlatformPkgX64\DEBUG_VS2019\X64\ShellPkg\Application\Shell\Shell\OUTPUT\Shell.efi $(BIN_DIR)\$(MODULE_NAME_GUID).efi
	-$(CP) $(DEBUG_DIR)\*.map $(OUTPUT_DIR)
	-$(CP) $(DEBUG_DIR)\*.pdb $(OUTPUT_DIR)

$(FFS_OUTPUT_DIR)\$(MODULE_GUID).map : E:\Code\ADL_N100_Code\UefiPldPlatform\Build\AdlpPlatformPkgX64\DEBUG_VS2019\X64\ShellPkg\Application\Shell\Shell\OUTPUT\Shell.map
	if exist E:\Code\ADL_N100_Code\UefiPldPlatform\Build\AdlpPlatformPkgX64\DEBUG_VS2019\X64\ShellPkg\Application\Shell\Shell\OUTPUT\Shell.map $(CP) E:\Code\ADL_N100_Code\UefiPldPlatform\Build\AdlpPlatformPkgX64\DEBUG_VS2019\X64\ShellPkg\Application\Shell\Shell\OUTPUT\Shell.map $(FFS_OUTPUT_DIR)\$(MODULE_GUID).map
