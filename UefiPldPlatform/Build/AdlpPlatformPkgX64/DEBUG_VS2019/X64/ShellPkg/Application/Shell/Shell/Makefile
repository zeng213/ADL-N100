	Trim --Vfr-Uni-Offset -o $(FFS_OUTPUT_DIR)\Shell.offset --ModuleName=Shell --DebugDir=E:\Code\ADL_N100_Code\UefiPldPlatform\Build\AdlpPlatformPkgX64\DEBUG_VS2019\X64\ShellPkg\Application\Shell\Shell\DEBUG 
