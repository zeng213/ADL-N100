	if exist $(FFS_OUTPUT_DIR)\Shell.offset GenSec -s EFI_SECTION_RAW -o $(FFS_OUTPUT_DIR)\ShellOffset.raw $(FFS_OUTPUT_DIR)\Shell.offset
